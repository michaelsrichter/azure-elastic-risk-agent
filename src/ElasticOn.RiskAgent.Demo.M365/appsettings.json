{
"AIServicesAgentID": "YOUR_AGENT_ID_HERE",
"AIServicesProjectEndpoint": "https://YOUR-PROJECT.services.ai.azure.com/api/projects/YOUR-PROJECT",
"AIServicesModelId": "gpt-4.1-mini",
"AIServicesAgentName": "RiskAgent",
"AIServicesAgentInstructions": "You answer questions about Risk based on what you find in the context.",
"AIServicesMCPToolServerLabel": "elastic_search_mcp",
"AIServicesMCPToolServerUrl": "https://YOUR-ELASTIC-CLUSTER.kb.eastus.azure.elastic.cloud/api/agent_builder/mcp",
"AIServicesMCPToolAllowedTools0": "azure_elastic_risk_agent_search_docs",
"AIServicesElasticApiKey": "YOUR_ELASTIC_API_KEY_HERE",
"AIServicesContentSafetyEndpoint": "https://YOUR-CONTENT-SAFETY.cognitiveservices.azure.com/",
"AIServicesContentSafetySubscriptionKey": "YOUR_CONTENT_SAFETY_SUBSCRIPTION_KEY_HERE",
"AIServicesContentSafetyJailbreakDetectionMode": "Enforce",
  "AgentApplicationOptions": {
    "StartTypingTimer": false,
    "RemoveRecipientMention": false,
    "NormalizeMentions": false
  },

  "TokenValidation": {
    "Audiences": [
      "{{ClientId}}" // this is the Client ID used for the Azure Bot
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Agents": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Connections": {
    "BotServiceConnection": {
      "Settings": {
        "AuthType": "UserManagedIdentity", // this is the AuthType for the connection, valid values can be found in Microsoft.Agents.Authentication.Msal.Model.AuthTypes.
        "ClientId": "{{BOT_ID}}", // this is the Client ID used for the connection.
        "TenantId": "{{BOT_TENANT_ID}}",
        "Scopes": [
          "https://api.botframework.com/.default"
        ]
      }
    }
  },
  "ConnectionsMap": [
    {
      "ServiceUrl": "*",
      "Connection": "BotServiceConnection"
    }
  ]
}
