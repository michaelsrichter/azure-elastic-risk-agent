{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/lib/*", "/*.{js,css,png,svg,jpg,jpeg,gif,ico}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  },
  "globalHeaders": {
    "cache-control": "no-cache, no-store, must-revalidate"
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ]
}
